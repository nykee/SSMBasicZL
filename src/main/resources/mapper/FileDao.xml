<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="biz.inno.dao.FileDao">
    <insert id="UploadFile" parameterType="String">
      INSERT INTO fileupload (originfilename,storefilename,path) VALUES (#{originfilename},#{storefilename},#{path})
    </insert>
</mapper>